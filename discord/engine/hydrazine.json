{
  "$CONFIGS": {
    "$verbose": true,
    "$logs": {
      "err": "../logs/err.log",
      "warn": "../logs/warn.log"
    }
  },
  "$ENV": {
    "key": "value"
  },
  "$GLOBAL": {
    "$PREFIX": "h!",
    "$PRESENCE": {
      "name": "Created With Hydrazine.",
      "type": "WATCHING"
    }
  },
  "$cinfo": {
    "$onInitListeners": [
      {
        "$type": "text_command",
        "$conditions": {
          "$equals": []
        },
        "$actions": [
          {
            "$type": "run_sqlite_query",
            "$name": "MySQL Query",
            "$callbacks": [
              {
                "$type": "console_log",
                "$fields": [
                  {
                    "$type": "string",
                    "$name": "Message",
                    "$placeholder": "$BOTNAME has started",
                    "$value": "$RESULT"
                  }
                ],
                "$actions": [
                  {
                    "$type": "console_log",
                    "$value": "$RESULT",
                    "$callbacks": [null]
                  }
                ]
              }
            ],
            "$placeholder": "SELECT * FROM users",
            "$value": ""
          }
        ]
      },
      {
        "$type": "process",
        "$conditions": {
          "$equals": []
        },
        "$actions": [
          {
            "$type": "console_log",
            "$name": "Message",
            "$callbacks": [],
            "$placeholder": "$BOTNAME has started",
            "$value": "$RESULT"
          }
        ]
      },
      {
        "$type": "process",
        "$conditions": {
          "$equals": []
        },
        "$actions": [
          {
            "$type": "console_log",
            "$name": "Message",
            "$callbacks": [],
            "$placeholder": "$BOTNAME has started",
            "$value": "$BOTNAME is Online!"
          }
        ]
      }
    ],
    "$listeners": [],
    "$actions": []
  }
}
